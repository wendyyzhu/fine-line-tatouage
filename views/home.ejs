<h2>Trending tattoos</h2>

<main>
    <% for (let tattoo of tattoos) {%>
    
        <a href="/tattoos/<%= tattoo.id %>"><img class = "grid-image" src="<%= tattoo.image_url %>" alt="<%= tattoo.title %>"></a>
        
    <% } %>
</main>

<h2>Newly added</h2>

<main>
    <% for (let newTattoo of newTattoos) {%>
    
        <a href="/tattoos/<%= newTattoo.id %>"><img class = "grid-image" src="<%= newTattoo.image_url %>" alt="<%= newTattoo.title %>"></a>
        
    <% } %>
</main>

